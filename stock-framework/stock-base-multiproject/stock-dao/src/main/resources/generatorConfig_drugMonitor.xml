<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- Mysql连接Jar包路径 -->
	<classPathEntry
		location="E:\maven-repository\mysql\mysql-connector-java\5.1.36\mysql-connector-java-5.1.36.jar" />
	<context id="DBMySQL" targetRuntime="MyBatis3">
		<!-- 抑制警告 -->
		<property name="suppressTypeWarnings" value="true" />
		<!-- generate entity时，生成hashcode和equals方法 -->
		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<!-- generate entity时，生成serialVersionUID -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<!-- 这个插件只会增加字符串字段映射到一个JDBC字符的方法 -->
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<!-- genenat entity时,生成toString -->
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />

		<!-- dijia.tang 自定义翻页插件 -->
		<!-- <plugin type="com.blsb.dao.page.MySQLPaginationPlugin_SYS"></plugin> -->

		<!-- 是否去除自动注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
			<property name="suppressDate" value="false" />
		</commentGenerator>

		<!-- Jdbc连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"    
            connectionURL="jdbc:mysql://47.92.39.161:3306/blsb?characterEncoding=utf8" 
            userId="blsb" password="blsb">
        </jdbcConnection>    
   
		<!-- Java类型转换 -->
		<!--将 JDBC DECIMAL和NUMERIC解析为Integer，而不是BigDecimal -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 生成Model类，targetProject:自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.blsb.dao.model.drugMonitor"
			targetProject="src/main/java">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
			<!-- 从数据库返回的值被清理前后的空格 -->
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- 生成XML映射文件 -->
		<sqlMapGenerator targetPackage="com.blsb.dao.mapper.drugMonitor"
			targetProject="src/main/resources">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<!-- 生成Mapper接口(DAO) -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.blsb.dao.mapper.drugMonitor" targetProject="src/main/java">
			<!-- enableSubPackages:是否让schema作为包的后缀 -->
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<!-- 要生成映射代码的数据表，tableName为表名；domainObjectName为对应的javaBean类名, enablexxxxx是与Example类相关的配置 -->

		 <table tableName="doctor_base_info" />
		 <table tableName="pharmacist_base_info" />
		 <table tableName="drug_base_info" />
		 <table tableName="comsume_base_info" />
		 <table tableName="drug_permit" />
		 <table tableName="drug_mix_permit" />
		 <table tableName="consume_permit" />
		 <table tableName="drug_repository_info" />
		 <table tableName="consume_repository_info" />
		 <table tableName="outpatient_recipe_drug_data" />
		 <table tableName="outpatient_recipe_consume_data" />
		 <table tableName="outpatient_mix_drug_data" />
		 <table tableName="inhos_recipe_drug_data" />
		 <table tableName="inhos_recipe_consume_data" />
		 <table tableName="inhos_mix_drug_data" />	
	</context>
</generatorConfiguration>